<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_0-0.dtd">
<sun-ejb-jar>
	
	<enterprise-beans>
			<!-- Imixs Workflow Service EJBs - JNDI Definition -->
		<ejb>
			<ejb-name>EntityServiceBean</ejb-name>
			<jndi-name> ejb/OfficeEntityServiceBean </jndi-name>
		</ejb>
		<ejb>
			<ejb-name>ModelServiceBean</ejb-name>
			<jndi-name>ejb/OfficeModelServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>WorkflowServiceBean</ejb-name>
			<jndi-name> ejb/OfficeWorkflowServiceBean
                        </jndi-name>
		</ejb>
		<ejb>
			<ejb-name>ReportServiceBean</ejb-name>
			<jndi-name> ejb/OfficeReportServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>WorkflowSchedulerBean</ejb-name>
			<jndi-name>ejb/OfficeWorkflowScheduler</jndi-name>
			<principal><name>WorkflowScheduler</name></principal>
		</ejb>


		<!-- #### Project specific EJBs ###-->
		
		<ejb>
			<ejb-name>WorkitemServiceBean</ejb-name>
			<jndi-name>ejb/OfficeWorkitemServiceBean</jndi-name>
		</ejb>

		<ejb>
			<ejb-name>ProjectServiceBean</ejb-name>
			<jndi-name>ejb/OfficeProjectServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>ProfileServiceBean</ejb-name>
			<jndi-name>ejb/OfficeProfileServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>ConfigServiceBean</ejb-name>
			<jndi-name>ejb/OfficeConfigServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>InvitationServiceBean</ejb-name>
			<jndi-name>ejb/OfficeInvitationServiceBean</jndi-name>
		</ejb>
		<ejb>
			<ejb-name>SequenceServiceBean</ejb-name>
			<jndi-name>ejb/OfficeSequenceServiceBean</jndi-name>
			<principal><name>WorkflowScheduler</name></principal>
		</ejb>
		
	
		</enterprise-beans>
</sun-ejb-jar>