<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<!-- Imixs Office Faces Config file -->


	
	<managed-bean>
		<managed-bean-name>contactMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.contact.ContactMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	
	<managed-bean>
		<managed-bean-name>searchMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.SearchMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	
	<managed-bean>
		<managed-bean-name>LdapMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.LdapMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	

	
	<managed-bean>
		<managed-bean-name>workitemReferenceMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>


	<managed-bean>
		<managed-bean-name>timeSheetMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.TimeSheetMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	
	
	<!-- Contact Lookup - 9000 -->
	<managed-bean>
		<managed-bean-name>contactLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>9...</value>
		</managed-property>
	</managed-bean>
	
	<!-- Project Lookup - 6000 -->
	<managed-bean>
		<managed-bean-name>projectLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>6...</value>
		</managed-property>
	</managed-bean>
		
	<!-- Contract Lookup - 4000 -->
	<managed-bean>
		<managed-bean-name>contractLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>4...</value>
		</managed-property>
	</managed-bean>
	
	<!-- Order Lookup - 7000 -->
	<managed-bean>
		<managed-bean-name>orderLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>7...</value>
		</managed-property>
	</managed-bean>
		
		
	<!-- outgoing invoice Lookup - 1000 -->
	<managed-bean>
		<managed-bean-name>outgoingInvoiceLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>1...</value>
		</managed-property>
	</managed-bean>	
	
	
		
	<!-- incomming invoice Lookup - 5000 -->
	<managed-bean>
		<managed-bean-name>incommingInvoiceLookupMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemReferenceMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<!-- Filter -->
		<managed-property>
			<property-name>filter</property-name>
			<property-class>java.lang.String</property-class>
			<value>5...</value>
		</managed-property>
	</managed-bean>	
		
		
	<managed-bean>
		<managed-bean-name>projectWizzardMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.project.ProjectWizzardMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>commentMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.CommentMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	
	<managed-bean>
		<managed-bean-name>modelManagerMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.admin.ModelManagerMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	<managed-bean>
		<managed-bean-name>portletUpdatelistMB</managed-bean-name>
		<managed-bean-class>org.imixs.marty.web.workitem.WorklistMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>count</property-name>
			<property-class>int</property-class>
			<value>5</value>
		</managed-property>
		<managed-property>
			<property-name>queryType</property-name>
			<property-class>int</property-class>
			<value>2</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>portletWorklistMB</managed-bean-name>
		<managed-bean-class>org.imixs.marty.web.workitem.WorklistMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>count</property-name>
			<property-class>int</property-class>
			<value>5</value>
		</managed-property>
		<managed-property>
			<property-name>queryType</property-name>
			<property-class>int</property-class>
			<value>0</value>
		</managed-property>
	</managed-bean>
	
	
	
	
	<managed-bean>
		<managed-bean-name>workitemHistoryMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.office.WorkitemHistoryMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>dmsMB</managed-bean-name>
		<managed-bean-class>
			org.imixs.marty.web.util.DmsMB
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	
	
	
	<navigation-rule>
	
		<navigation-case>
			<from-outcome>contact_ok</from-outcome>
			<to-view-id>/contact_ok.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>wizzard</from-outcome>
			<to-view-id>/pages/welcome.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		
		<navigation-case>
			<from-outcome>startprocess</from-outcome>
			<to-view-id>/pages/startprocess.xhtml
			</to-view-id>
			
		</navigation-case>
		
		<navigation-case>
			<from-outcome>open_my_settings</from-outcome>
			<to-view-id>/pages/profile/settings.xhtml
			</to-view-id>
			
		</navigation-case>
		
		
		
		
		<navigation-case>
			<from-outcome>open_searchlist</from-outcome>
			<to-view-id>/pages/workitems/searchlist.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		<!-- *** Administration *** -->
		<navigation-case>
			<from-outcome>admin_profilelist</from-outcome>
			<to-view-id>/pages/admin/profilelist.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin_profile</from-outcome>
			<to-view-id>/pages/admin/profile.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin_config</from-outcome>
			<to-view-id>/pages/admin/config.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin_projectlist</from-outcome>
			<to-view-id>/pages/admin/projectlist.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin_project</from-outcome>
			<to-view-id>/pages/admin/project.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		
		<navigation-case>
			<from-outcome>admin_model</from-outcome>
			<to-view-id>/pages/admin/modellist.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		<!-- DMS -->
		<navigation-case>
			<from-outcome>admin_dmsscheduler</from-outcome>
			<to-view-id>/pages/admin/dmsscheduler.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>admin_dmsassign</from-outcome>
			<to-view-id>/pages/workitems/forms/dms_assignprocess.xhtml
			</to-view-id>
			<redirect/>
		</navigation-case>
		
		
	</navigation-rule>
</faces-config>